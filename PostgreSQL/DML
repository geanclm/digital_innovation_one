CREATE TABLE IF NOT EXISTS public.teste (
	id SERIAL PRIMARY KEY,
	nome VARCHAR(50) NOT NULL,
	created_at TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP	
)

DROP TABLE IF EXISTS teste;

CREATE TABLE IF NOT EXISTS public.teste (
	nome VARCHAR(50) NOT NULL,
	cpf VARCHAR(11) NOT NULL,
	created_at TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (cpf)
);

INSERT INTO teste (cpf, nome, created_at)
VALUES ('91067073949', 'Gean Carlos','2021-07-23 23:00:00') ON CONFLICT (cpf) DO NOTHING;

UPDATE teste SET nome ='Gean CArlos Luciano Madeira' WHERE cpf='91067073949'

SELECT * FROM teste;
